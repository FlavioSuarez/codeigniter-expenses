{
    "name": "neznaika0/codeigniter-expenses",
    "description": "CodeIgniter Expenses",
    "license": "MIT",
    "type": "project",
    "homepage": "https://expenses.ozornick.ru",
    "support": {
        "source": "https://github.com/neznaika0/codeigniter-expenses"
    },
    "require": {
        "php": "^8.1",
        "codeigniter4/framework": "^4.5.2",
        "codeigniter4/translations": "^4.5.1",
        "twbs/bootstrap": "5.3.1"
    },
    "require-dev": {
        "codeigniter/coding-standard": "^1.7.16",
        "fakerphp/faker": "^1.23.1",
        "friendsofphp/php-cs-fixer": "^3.58.1",
        "mikey179/vfsstream": "^1.6.11",
        "phpunit/phpunit": "^9.6.19"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "exclude-from-classmap": [
            "**/Database/Migrations/**"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Support\\": "tests/_support"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "scripts": {
        "test": "phpunit"
    }
}
